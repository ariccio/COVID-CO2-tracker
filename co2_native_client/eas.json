{
  "cli": {
    "version": ">= 0.47.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "buildArtifactPaths": "ios/build/*"

      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
